{
    "name": "InstaBlocker",
    "version": "0.1.0",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.instagram.com/*"
            ],
            "js": [
                "contents.js"
            ]
        }
    ],
    "manifest_version": 3
}